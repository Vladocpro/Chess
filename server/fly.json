{
   "app": "chessdeploy",
   "primary_region": "lhr",
   "http_service": {
      "internal_port": 3000,
      "force_https": true,
      "auto_stop_machines": true,
      "auto_start_machines": true,
      "min_machines_running": 1,
      "processes": [
         "app"
      ]
   },
   "vm": [
      {
         "memory": "256mb",
         "cpu_kind": "shared",
         "cpus": 1
      }
   ]
}
